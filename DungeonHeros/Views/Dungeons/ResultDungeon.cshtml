@model DungeonHeros.Models.ViewModels.ResultViewModel.ResultViewModel

@{
    ViewBag.Title = "title";
    Layout = "_Layout";
}
<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.01//EN" "http://www.w3.org/TR/html4/strict.dtd">
<html lang="en">
<head>
    <meta http-equiv="Content-Type" content="text/html;charset=UTF-8">
    <title>Results</title>
</head>
<body class="bg-dark mb-5">
<div class="container-md mt-5">
    <div class="card text-center bg-body">
        <div class="card-header bg-body">
        </div>
        <div class="card-body">
            <h5 class="card-title text-white">@Model.Title</h5>
            <p class="card-text text-white">@Model.Corpse</p>
            <a asp-controller="Home" asp-action="Index" class="btn btn-secondary">Back to main menu</a>
        </div>
    </div>
</div>
</body>
</html>